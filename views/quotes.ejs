<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Results!</title>
  </head>
  <body>
    <h2>Here are the awesome quotes!</h2>
    <% if(typeof(errors) != 'undefined' ) { %>
        <% for (var x in errors) { %>
         <h3><%= errors[x].message %></h3>
        <% } %>
    <% } %>

    <% if(quotes.length > 0) { %>
        <% for (var x in quotes) { %>
          <p>
          &quot;<%= quotes[x].quote %>&quot;
          </p>
         <p>- <%= quotes[x].name %> at <%= quotes[x].createdAt %></p>
         <hr />
        <% } %>
    <% } %>

    <a href="/">Go Back</a>
  </body>
</html>